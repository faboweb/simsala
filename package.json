{
  "name": "simsala",
  "version": "0.0.12",
  "description": "Conflict free changelogs and release management",
  "main": "src/cli.js",
  "scripts": {
    "start": "node src/cli.js",
    "lint": "prettier *.js --write",
    "test": "jest"
  },
  "keywords": [
    "changelog",
    "conflict",
    "release",
    "version"
  ],
  "author": "faboweber@gmail.com",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "commander": "^2.20.0",
    "inquirer": "^6.2.2",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bin": {
    "simsala": "./src/cli.js"
  }
}
